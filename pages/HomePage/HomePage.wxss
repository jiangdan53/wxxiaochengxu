.ml{
  display: inline-block;
  padding: 10rpx 20rpx;
  border: 1rpx solid #000;
  font-size: 28rpx
}
.nva{
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
}
.data{
  margin-top: 100rpx;
  display: block;
}
.londings{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background-color: #fff;
}
.lon{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%)
}